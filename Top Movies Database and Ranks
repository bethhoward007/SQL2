#Use a select statement to find the SUM,MIN,AVG of the following stats(Domestic,OverseasPct,and Worldwide).
SELECT * FROM topmovies;

SELECT SUM(Domestic), AVG(OverseasPct), MIN(Worldwide) FROM topmovies;

#Select movies by year in the data (using HAVING)?

SELECT * FROM topmovies GROUP BY title HAVING year < 2000;

#Find most popular movies by rank and setting parameters on those said ranks.
SELECT Title, Rank
FROM topmovies WHERE Rank >= 60 AND Rank <= 30;

#Write a clause to group the data values that donâ€™t exist yet (using CASE).

SELECT Title, Rank,
CASE 
WHEN Rank <= 30 THEN "toptier_movies"
WHEN Rank >= 60 THEN "midtier_movies"
ELSE "lowtier_movies"
END as "Movie Tiers"
FROM topmovies 
GROUP BY Rank;

#Use a slect statment involving AND or OR.

SELECT * 
FROM topmovies 
WHERE Rank >= 50 AND Year <= 2010;
